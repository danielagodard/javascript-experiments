var space = " ";
 
var myIntervalTimer = setInterval(function(){ // Runs once every 10 milliseconds.
  space += " ";
  if(space.length > 20){
    space = " ";
  }
  if(flipCoin()){
    console.log('%c'+space, 'background: red; font-size: '+Math.round(Math.random()*50)+'px;');
  } else {
    console.log('%c'+space, 'background: lime; font-size: '+Math.round(Math.random()*50)+'px;');
  }
}, 10);
 
setTimeout(function(){ // In 10 seconds we stop setInterval from running.
  clearInterval(myIntervalTimer);
}, 1000 * 10);






var flipCoin = function(){ // Returns true or false
  if(Math.random() > 0.5){
    return true;
  } else {
    return false;
  }
}
 
// setTimeout runs a given function after a certain time period.
// clearTimeout(x) stops a timer before it fires.
 
// clearInterval(x) stops a looping timer.
// setInterval runs a given function after a certain time period in a loop.
 
var space = " ";
 
var myIntervalTimer = setInterval(function(){ // Runs once every 10 milliseconds.
  space += " ";
  if(space.length > 10){
    space = " ";
  }
  if(flipCoin()){
    console.log("%c ++☺☺☺☺☺☺☺☺☺☺☺☺☺☺☺☺☺☺☺☺☺☺☺☺☺☺☺☺☺☺☺☺☺☺☺☺☺☺", "font-size: 40px; color: black; background: yellow");
  } else {
    console.log("%c ++MY NAME IS DANIELA GODARD RANGEL", "font-size: 20px; color: blue; background: yellow");
  } 
}, 10);
 

setTimeout(function(){ // In 10 seconds we stop setInterval from running.
  clearInterval(myIntervalTimer);
}, 1000 * 20);
